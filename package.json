{
  "name": "better-admission-list",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all build -p watch:*",
    "start:production": "node build/server/index.js",
    "build": "npm-run-all build:clean -p build:server build:static",
    "build:clean": "rm -R build || :",
    "build:static": "copyfiles -e './**/index.html' -u 1 ./static/**/* build/static/",
    "build:server": "tsc --project server",
    "build:client": "webpack --config build/webpack.config.js",
    "build:production": "run-s build build:client",
    "watch:server-typescript": "tsc --project server --watch",
    "watch:server-build": "nodemon --watch build/server build/server/index.js",
    "lint": "eslint --ext=.ts,.tsx ./",
    "lint:fix": "eslint --fix --ext=.ts,.tsx ./"
  },
  "dependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@koa/cors": "^3.1.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.45",
    "@svgr/webpack": "^5.3.0",
    "@types/axios": "^0.14.0",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/koa": "^2.11.0",
    "@types/koa-compose": "^3.2.5",
    "@types/koa-compress": "^4.0.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-router": "^7.0.42",
    "@types/koa-static": "^4.0.1",
    "@types/koa-webpack": "^5.0.1",
    "@types/node": "^12.12.26",
    "@types/node-fetch": "^2.5.5",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "@types/react-infinite-scroller": "^1.2.1",
    "@types/react-router-dom": "^5.1.3",
    "@types/webpack": "^4.41.8",
    "axios": "^0.19.2",
    "axios-hooks": "^2.1.0",
    "babel-loader": "^8.0.6",
    "config": "^3.2.4",
    "config-webpack": "^1.0.4",
    "copyfiles": "^2.1.1",
    "core-js": "^3.6.4",
    "dotenv": "^8.2.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "html-table-to-json": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compose": "^4.1.0",
    "koa-compress": "^5.0.1",
    "koa-logger": "^3.2.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "koa-webpack": "^5.2.4",
    "material-table": "^1.66.0",
    "node-fetch": "^3.1.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
